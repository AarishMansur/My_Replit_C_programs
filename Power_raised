 WAP to find x raised to power y
 #include<stdio.h>
 int main(){
    int x,y,power;
   power=1;
   printf("Enter the value of x:");
   scanf("%d",&x);
   printf("Enter the value  of y:");
   scanf("%d",&y);
   for(int i=1;i<=y;i++){
     power*= x;
   }
   printf("%d raised to the power of %d = %d ",x,y,power);
 }
